{
    "version": 2,
    "framework": null,
    "builds": [
        { "src": "public/index.php", "use": "vercel-php@0.7.1" },
        { "src": "public/**", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/(css|js|images|storage)/(.*)", "dest": "/public/$1/$2" },
        { "src": "/(.*)", "dest": "/public/index.php" }
    ]
}